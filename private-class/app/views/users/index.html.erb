<%= link_to 'New Student', new_user_session_path  %>


<%= @user.each do |f| %>
<div>
    <%= f.text_field :first_name, placeholder: 'First Name' %>
</div>
<div>
    <%= f.text_field :last_name, placeholder: 'Last Name' %>
</div>
<div>
    <%= f.text_field :phone, placeholder: 'Phone' %>
</div>

    

<%= f.submit 'Save' %>
<% end %>

